{% comment %} Template: TrailVision Pro X50 Landing Page {% endcomment %}
<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title data-meta="page_title">{{ template_data.page_title }}</title>

    <link
      href="https://fonts.googleapis.com/css2?family=Manrope:wght@400;500;600;700;800&family=Space+Grotesk:wght@500;600;700&display=swap"
      rel="stylesheet"
    >

    <link
      rel="stylesheet"
      href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/6.4.0/css/all.min.css"
    >

    <script src="https://cdn.tailwindcss.com"></script>

    <style>
      * {
        font-family: 'Manrope', sans-serif;
      }

      .font-space {
        font-family: 'Space Grotesk', sans-serif;
      }

      .hero-container {
        display: grid;
        gap: 2rem;
      }

      @media (min-width: 1024px) {
        .hero-container {
          grid-template-columns: 1fr 1fr;
          gap: 4rem;
          align-items: center;
        }

        .hero-badges {
          grid-column: 1;
        }

        .hero-headline {
          grid-column: 1;
        }

        .hero-subheadline {
          grid-column: 1;
        }

        .hero-features {
          grid-column: 1;
        }

        .hero-cta {
          grid-column: 1;
        }

        .hero-social {
          grid-column: 1;
        }

        .hero-image {
          grid-column: 2;
          grid-row: 1 / span 7;
          align-self: center;
        }

        .hero-image::before {
          content: '';
          position: absolute;
          inset: -2rem;
          background: radial-gradient(circle, rgba(16, 185, 129, 0.2), transparent);
          filter: blur(3rem);
          z-index: -1;
        }
      }

      @media (max-width: 1023px) {
        .hero-container {
          display: flex;
          flex-direction: column;
          gap: 1.5rem;
        }

        .hero-badges {
          order: 1;
        }

        .hero-headline {
          order: 2;
        }

        .hero-subheadline {
          order: 3;
        }

        .hero-image {
          order: 4;
          margin: 1rem 0;
        }

        .hero-features {
          order: 5;
        }

        .hero-cta {
          order: 6;
        }

        .hero-social {
          order: 7;
        }
      }

      @keyframes slideDown {
        from {
          transform: translateY(-100%);
          opacity: 0;
        }
        to {
          transform: translateY(0);
          opacity: 1;
        }
      }

      @keyframes fadeInUp {
        from {
          transform: translateY(20px);
          opacity: 0;
        }
        to {
          transform: translateY(0);
          opacity: 1;
        }
      }

      @keyframes glow {
        0%,
        100% {
          box-shadow: 0 0 20px rgba(34, 197, 94, 0.5);
        }
        50% {
          box-shadow: 0 0 30px rgba(34, 197, 94, 0.8);
        }
      }

      .animate-slideDown {
        animation: slideDown 0.5s ease-out;
      }

      .animate-fadeInUp {
        animation: fadeInUp 0.8s ease-out;
      }

      .glow-effect {
        animation: glow 2s ease-in-out infinite;
      }

      html {
        scroll-behavior: smooth;
      }

      .progress-smooth {
        transition: width 0.3s cubic-bezier(0.4, 0, 0.2, 1);
      }

      .gradient-text {
        background: linear-gradient(135deg, #059669 0%, #10b981 100%);
        -webkit-background-clip: text;
        -webkit-text-fill-color: transparent;
        background-clip: text;
      }

      .premium-shadow {
        box-shadow: 0 20px 25px -5px rgba(0, 0, 0, 0.1), 0 10px 10px -5px rgba(0, 0, 0, 0.04);
      }

      @keyframes slideInLeft {
        from {
          transform: translateX(-120%);
        }
        to {
          transform: translateX(0);
        }
      }

      .notification-slide {
        animation: slideInLeft 0.5s ease-out;
      }

      @media (max-width: 640px) {
        section {
          padding-left: 1rem;
          padding-right: 1rem;
        }

        h1 {
          line-height: 1.15 !important;
        }

        .container {
          padding-left: 1rem;
          padding-right: 1rem;
        }
      }
    </style>
    <script>
      window.productData = {
            id: '{{ product.id }}',
            title: '{{ product.title | escape }}',
            handle: '{{ product.handle }}',
            price: {{ product.price }},
            compare_at_price: {% if product.compare_at_price %}{{ product.compare_at_price }}{% else %}''{% endif %},
            available: {{ product.available }},
            image: "{{ product.featured_image | image_url: width: 100 }}",
            variants: [
              {% for variant in product.variants %}
              {
                id: {{ variant.id }},
                title: "{{ variant.title | escape }}",
                price: {{ variant.price }},
                available: {{ variant.available }},
                quantity: {{ variant.inventory_quantity }},
                image: {% if variant.image %}"{{ variant.image | image_url: width: 100 }}"{% else %}""{% endif %}
              }{% unless forloop.last %},{% endunless %}
              {% endfor %}
            ],
            options: {{ product.options_with_values | json }},
            selected_or_first_available_variant: {{ product.selected_or_first_available_variant | json }},
            featuredImage: {
              url: '{{ product.featured_image | image_url: width: 1000 }}'
            },
            collections: {{ product.collections | json }}
          };
    </script>
    {% assign template_data = product.metafields.custom.template_data.value | parse_json %}
  </head>
  <body class="bg-slate-50">
    <div class="fixed top-0 left-0 w-full h-1 bg-slate-200 z-[100]">
      <div
        id="progress-bar"
        class="h-full bg-gradient-to-r from-emerald-500 to-green-500 progress-smooth"
        style="width: 0%"
      ></div>
    </div>

    <header
