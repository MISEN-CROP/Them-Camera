    <script>
      let scrollPosition = 0;

      function openPopup(popupType) {
        scrollPosition = window.pageYOffset;

        const popup = document.getElementById(popupType + '-popup');
        if (popup) {
          popup.style.display = 'flex';
          document.body.style.position = 'fixed';
          document.body.style.top = `-${scrollPosition}px`;
          document.body.style.width = '100%';
        }
      }

      function closePopup(popupType) {
        const popup = document.getElementById(popupType + '-popup');
        if (popup) {
          popup.style.display = 'none';
          document.body.style.position = '';
          document.body.style.top = '';
          document.body.style.width = '';
          window.scrollTo(0, scrollPosition);
        }
      }

      document.querySelectorAll('[id$="-popup"]').forEach((popup) => {
        popup.addEventListener('click', function (e) {
          if (e.target === this) {
            const popupType = this.id.replace('-popup', '');
            closePopup(popupType);
          }
        });
      });

      document.addEventListener('DOMContentLoaded', function () {
        document.querySelectorAll('a[href^="#"]').forEach((anchor) => {
          anchor.addEventListener('click', function (e) {
            e.preventDefault();
            const targetId = this.getAttribute('href');
            if (targetId && targetId !== '#') {
              const target = document.querySelector(targetId);
              if (target) {
                const headerOffset = 80;
                const elementPosition = target.getBoundingClientRect().top;
                const offsetPosition = elementPosition + window.pageYOffset - headerOffset;

                window.scrollTo({
                  top: offsetPosition,
                  behavior: 'smooth',
                });
              }
            }
          });
        });
      });

      document.querySelector('footer input[type="email"]').addEventListener('keypress', function (e) {
        if (e.key === 'Enter') {
          e.preventDefault();
          const email = this.value;
          if (email && email.includes('@')) {
            alert('Thank you for subscribing! Check your email for confirmation.');
            this.value = '';
          }
        }
      });

      document.querySelector('footer button').addEventListener('click', function (e) {
        const emailInput = this.previousElementSibling;
        const email = emailInput.value;
        if (email && email.includes('@')) {
          alert('Thank you for subscribing! Check your email for confirmation.');
          emailInput.value = '';
        }
      });

      console.log('TrailVision Pro X50 Landing Page Loaded Successfully');
    </script>
  </body>
</html>
