    <script>
      document.getElementById('menu-toggle').addEventListener('click', function () {
        const mobileNav = document.getElementById('mobile-nav');
        if (mobileNav.style.display === 'none' || mobileNav.style.display === '') {
          mobileNav.style.display = 'block';
        } else {
          mobileNav.style.display = 'none';
        }
      });

      window.addEventListener('scroll', function () {
        const scrolled = (window.scrollY / (document.documentElement.scrollHeight - window.innerHeight)) * 100;
        document.getElementById('progress-bar').style.width = scrolled + '%';

        const stickyCta = document.getElementById('sticky-cta');
        if (scrolled > 50) {
          stickyCta.style.display = 'block';
        } else {
          stickyCta.style.display = 'none';
        }
      });

      document.querySelectorAll('a[href^="#"]').forEach((anchor) => {
        anchor.addEventListener('click', function (e) {
          e.preventDefault();
          const target = document.querySelector(this.getAttribute('href'));
          if (target) {
            target.scrollIntoView({ behavior: 'smooth', block: 'start' });
          }
        });
      });

      function scrollToNext() {
        const nextSection = document.getElementById('technology');
        if (nextSection) {
          nextSection.scrollIntoView({ behavior: 'smooth', block: 'start' });
        }
      }

      let currentNotification = 1;
      const totalNotifications = 5;

      function showNotification() {
        for (let i = 1; i <= totalNotifications; i++) {
          document.getElementById('notification-' + i).style.display = 'none';
        }

        const notification = document.getElementById('notification-' + currentNotification);
        notification.style.display = 'block';

        setTimeout(function () {
          notification.style.display = 'none';
        }, 5000);

        currentNotification = (currentNotification % totalNotifications) + 1;
      }

      setTimeout(function () {
        showNotification();
        setInterval(showNotification, 60000 + Math.random() * 30000);
      }, 30000);

      setTimeout(function () {
        const reviewNotification = document.getElementById('review-notification');
        reviewNotification.style.display = 'block';
        setTimeout(function () {
          reviewNotification.style.display = 'none';
        }, 6000);
      }, 45000);

      let popupShown = false;
      let timeOnPage = 0;
      let scrollDepth = 0;

      setInterval(function () {
        timeOnPage++;
      }, 1000);

      window.addEventListener('scroll', function () {
        const scrolled = (window.scrollY / (document.documentElement.scrollHeight - window.innerHeight)) * 100;
        scrollDepth = Math.max(scrollDepth, scrolled);
      });

      document.addEventListener('mouseleave', function (e) {
        if (e.clientY <= 0 && !popupShown && timeOnPage >= 30 && scrollDepth >= 40) {
          const popup = document.getElementById('popup-overlay');
          popup.style.display = 'flex';
          popupShown = true;
        }
      });

      window.addEventListener('popstate', function (e) {
        if (!popupShown && timeOnPage >= 30 && scrollDepth >= 40) {
          e.preventDefault();
          const popup = document.getElementById('popup-overlay');
          popup.style.display = 'flex';
          popupShown = true;
          history.pushState(null, null, window.location.href);
        }
      });

      function closePopup() {
        document.getElementById('popup-overlay').style.display = 'none';
      }

      function goToStep2() {
        document.getElementById('popup-step-1').style.display = 'none';
        document.getElementById('popup-step-2').style.display = 'block';
      }

      function goToStep3() {
        const email = document.getElementById('email-input').value;
        if (email && email.includes('@')) {
          document.getElementById('popup-step-2').style.display = 'none';
          document.getElementById('popup-step-3').style.display = 'block';
        } else {
          alert('Please enter a valid email address');
        }
      }

      function copyCode() {
        navigator.clipboard.writeText('{{ template_data.popup_code }}').then(function () {
          const codeDiv = event.currentTarget;
          const originalHTML = codeDiv.innerHTML;
          codeDiv.innerHTML =
            '<p class="font-bold text-2xl text-green-600 mb-2"><i class="fas fa-check mr-2"></i>Copied!</p><p class="text-sm text-slate-600">Code saved to clipboard</p>';
          setTimeout(function () {
            codeDiv.innerHTML = originalHTML;
          }, 2000);
        });
      }

      function shopWithDiscount() {
        closePopup();
        const packagesSection = document.getElementById('packages');
        if (packagesSection) {
          packagesSection.scrollIntoView({
            behavior: 'smooth',
            block: 'start',
          });
        }
      }

      function playDemo() {
        alert('Demo video would play here');
      }

      document.getElementById('popup-close-btn').addEventListener('click', function () {
        document.getElementById('popup-overlay').style.display = 'none';
      });

      document.getElementById('popup-overlay').addEventListener('click', function (e) {
        if (e.target === this) {
          closePopup();
        }
      });
    </script>

